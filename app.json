{
  "expo": {
    "name": "UW Hub",
    "slug": "UWHub",
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./app/assets/logos/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./app/assets/logos/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#28292c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logos/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "androidNavigationBar": {
      "backgroundColor": "#28292c"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "translucent": true,
      "backgroundColor": "#00000000"
    },
    "web": {
      "favicon": "./app/assets/logos/favicon.png"
    }
  }
}
